                            </children>
                        </HBox>
                        <HBox prefHeight="20.0" prefWidth="219.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Ilość energii:"/>
                                <Text fx:id="watchingAnimalEnergyText" strokeType="OUTSIDE" strokeWidth="0.0" text="-"/>
                            </children>
                        </HBox>
                        <HBox prefHeight="20.0" prefWidth="219.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Liczb zjedzonych roślin:"/>
                                <Text fx:id="watchingAnimalEatenGrassCounterText" strokeType="OUTSIDE" strokeWidth="0.0"
                                      text="-"/>
                            </children>
                        </HBox>
                        <HBox prefHeight="20.0" prefWidth="219.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Liczba dzieci:"/>
                                <Text fx:id="watchingAnimalChildrenCounterText" strokeType="OUTSIDE" strokeWidth="0.0"
                                      text="-"/>
                            </children>
                        </HBox>
                        <HBox prefHeight="20.0" prefWidth="219.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Liczba potomków:"/>
                                <Text fx:id="watchingAnimalDescendantCounterText" strokeType="OUTSIDE" strokeWidth="0.0"
                                      text="-"/>
                            </children>
                        </HBox>
                        <HBox prefHeight="26.0" prefWidth="247.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Ile dni żyje:"/>
                                <Text fx:id="watchingAnimalLifetimeText" strokeType="OUTSIDE" strokeWidth="0.0"
                                      text="-"/>
                            </children>
                        </HBox>
                        <HBox prefHeight="26.0" prefWidth="247.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Którego dnia zmarło:"/>
                                <Text fx:id="watchingAnimalDeadDayText" strokeType="OUTSIDE" strokeWidth="0.0"
                                      text="-"/>
                            </children>
                        </HBox>
                    </children>
                </VBox>
                <Button fx:id="endGameButton" layoutX="14.0" layoutY="753.0" mnemonicParsing="false" onAction="#endGame"
