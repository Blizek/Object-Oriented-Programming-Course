            fx:controller="projekt.presenter.GameMapPresenter">
    <children>
        <AnchorPane fx:id="gameStatsPane" prefHeight="821.0" prefWidth="274.0">
            <children>
                <Text layoutX="85.0" layoutY="36.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Statystyki rozgrywki"/>
                <VBox layoutX="11.0" layoutY="48.0" prefHeight="231.0" prefWidth="249.0">
                    <children>
                        <HBox prefHeight="20.0" prefWidth="219.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Łączna liczba zwierząt:"/>
                                <Text fx:id="animalsCounterText" strokeType="OUTSIDE" strokeWidth="0.0"/>
                            </children>
                        </HBox>
                        <HBox prefHeight="20.0" prefWidth="219.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Łączna liczba traw:"/>
                                <Text fx:id="grassCounterText" strokeType="OUTSIDE" strokeWidth="0.0"/>
                            </children>
                        </HBox>
                        <HBox prefHeight="20.0" prefWidth="219.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Liczba wolnych pól:"/>
                                <Text fx:id="freePlacesCounterText" strokeType="OUTSIDE" strokeWidth="0.0"/>
                            </children>
                        </HBox>
                        <HBox prefHeight="20.0" prefWidth="219.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Najpopularniejszy genotyp:"/>
                                <Text fx:id="mostPopularGenText" strokeType="OUTSIDE" strokeWidth="0.0"/>
                            </children>
                        </HBox>
                        <HBox prefHeight="20.0" prefWidth="219.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Średnia energia zwierząt:"/>
                                <Text fx:id="averageAnimalsEnergyText" strokeType="OUTSIDE" strokeWidth="0.0"/>
                            </children>
                        </HBox>
                        <HBox prefHeight="20.0" prefWidth="219.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Średnia długość życia zwierząt:"/>
                                <Text fx:id="averageAnimalsLifetimeText" strokeType="OUTSIDE" strokeWidth="0.0"/>
                            </children>
                        </HBox>
                        <HBox prefHeight="26.0" prefWidth="247.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Średnia liczba dzieci zwierząt:"/>
                                <Text fx:id="averageAnimalsChildrenText" strokeType="OUTSIDE" strokeWidth="0.0"/>
                            </children>
                        </HBox>
                    </children>
                </VBox>
                <AnchorPane layoutX="11.0" layoutY="290.0" prefHeight="247.0" prefWidth="249.0">
                    <children>
                        <LineChart fx:id="animalsChart" layoutX="-2.0" prefHeight="231.0" prefWidth="250.0">
                            <xAxis>
                                <CategoryAxis prefHeight="19.0" prefWidth="206.0" side="BOTTOM"
                                              tickLabelFill="#ffffff00"/>
                            </xAxis>
                            <yAxis>
                                <NumberAxis side="LEFT"/>
                            </yAxis>
                        </LineChart>
                        <Text layoutX="11.0" layoutY="240.0" strokeType="OUTSIDE" strokeWidth="0.0"
                              text="Wykres liczby zwierzaków na mapie"/>
                    </children>
                </AnchorPane>
                <AnchorPane layoutX="13.0" layoutY="553.0" prefHeight="247.0" prefWidth="249.0">
                    <children>
                        <LineChart fx:id="grassesChart" prefHeight="231.0" prefWidth="250.0">
                            <xAxis>
                                <CategoryAxis prefHeight="11.0" prefWidth="8.0" side="BOTTOM" tickLabelFill="#ffffff00">
                                    <tickLabelFont>
                                        <Font/>
                                    </tickLabelFont>
                                </CategoryAxis>
                            </xAxis>
                            <yAxis>
                                <NumberAxis prefHeight="208.0" prefWidth="25.0" side="LEFT"/>
                            </yAxis>
                        </LineChart>
                        <Text layoutX="37.0" layoutY="242.0" strokeType="OUTSIDE" strokeWidth="0.0"
                              text="Wykres liczby traw na mapie"/>
                    </children>
                </AnchorPane>
            </children>
        </AnchorPane>
        <AnchorPane layoutX="274.0" prefHeight="821.0" prefWidth="812.0">
            <children>
                <AnchorPane prefHeight="68.0" prefWidth="812.0">
