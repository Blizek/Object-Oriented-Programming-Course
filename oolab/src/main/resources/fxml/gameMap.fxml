                        <Text fx:id="mapIdText" layoutX="482.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0"/>
                    </children>
                </AnchorPane>
                <AnchorPane layoutY="68.0" prefHeight="753.0" prefWidth="812.0">
                    <children>
                        <ScrollPane prefHeight="753.0" prefWidth="812.0">
                            <content>
                                <GridPane fx:id="mapGrid" alignment="CENTER" hgap="5.0" prefHeight="753.0"
                                          prefWidth="812.0" vgap="5.0">
                                </GridPane>
                            </content>
                        </ScrollPane>
                    </children>
                </AnchorPane>

            </children>
        </AnchorPane>
        <AnchorPane layoutX="1086.0" prefHeight="821.0" prefWidth="274.0">
            <children>
                <ImageView fx:id="watchingAnimalImage" fitHeight="160.0" fitWidth="160.0" layoutX="57.0" layoutY="63.0"
                           pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../img/question-mark.png"/>
                    </image>
                </ImageView>
                <Text layoutX="70.0" layoutY="42.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Obserwowany zwierzak"/>
                <VBox layoutX="13.0" layoutY="245.0" prefHeight="328.0" prefWidth="249.0">
                    <children>
                        <HBox prefHeight="20.0" prefWidth="219.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Pozycja:"/>
                                <Text fx:id="watchingAnimalPositionText" strokeType="OUTSIDE" strokeWidth="0.0"
                                      text="-"/>
                            </children>
                        </HBox>
                        <HBox prefHeight="20.0" prefWidth="219.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Kierunek:"/>
                                <Text fx:id="watchingAnimalDirection" strokeType="OUTSIDE" strokeWidth="0.0" text="-"/>
                            </children>
                        </HBox>
                        <HBox prefHeight="20.0" prefWidth="219.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Genom:"/>
                                <Text fx:id="watchingAnimalGenomeText" strokeType="OUTSIDE" strokeWidth="0.0" text="-"/>
                            </children>
                        </HBox>
                        <HBox prefHeight="20.0" prefWidth="219.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Aktualny gen:"/>
                                <Text fx:id="watchingAnimalActualGenText" strokeType="OUTSIDE" strokeWidth="0.0"
